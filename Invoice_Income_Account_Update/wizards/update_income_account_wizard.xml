<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_update_income_account_wizard_form" model="ir.ui.view">
        <field name="name">account.move.update.income.account.wizard.form</field>
        <field name="model">account.move.update.income.account.wizard</field>
        <field name="arch" type="xml">
            <form string="Update Income Account">
                <group>
                    <field name="move_id" invisible="1"/>
                    <field name="company_id" invisible="1"/>
                    <field name="income_account_id"/>
                </group>
                <footer>
                    <button string="Update" name="action_update_income_account" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>